<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.smart.dialogsamples"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.smart.dialogsamples.ui.SimpleHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.smart.dialogsamples.ui.DialogHomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity
            android:name="com.smart.dialogsamples.ui.PopupHomeActivity"/>
        <activity
            android:name="com.smart.dialogsamples.ui.BubblePopupActivity"/>
        <activity
            android:name="com.smart.dialogsamples.ui.CustomPopupActivity"/>
    </application>

</manifest>
